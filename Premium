            game:GetService("ReplicatedStorage").jdskhfsIIIllliiIIIdchgdIiIIIlIlIli:FireServer(dragonGiraffe.Humanoid, 1)
        end

        wait() -- Delay para evitar sobrecarga e reduzir a frequ√™ncia das verifica√ß√µes
    end
end

